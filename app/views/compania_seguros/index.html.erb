<p id="notice"><%= notice %></p>

<h1>Listing Compania Seguros</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Rut</th>
      <th>Dv</th>
      <th>Banco</th>
      <th>Cuenta bancaria</th>
      <th>Aux1</th>
      <th>Aux2</th>
      <th>Aux3</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @compania_seguros.each do |compania_seguro| %>
      <tr>
        <td><%= compania_seguro.nombre %></td>
        <td><%= compania_seguro.rut %></td>
        <td><%= compania_seguro.dv %></td>
        <td><%= compania_seguro.banco %></td>
        <td><%= compania_seguro.cuenta_bancaria %></td>
        <td><%= compania_seguro.aux1 %></td>
        <td><%= compania_seguro.aux2 %></td>
        <td><%= compania_seguro.aux3 %></td>
        <td><%= compania_seguro.image %></td>
        <td><%= link_to 'Show', compania_seguro %></td>
        <td><%= link_to 'Edit', edit_compania_seguro_path(compania_seguro) %></td>
        <td><%= link_to 'Destroy', compania_seguro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Compania seguro', new_compania_seguro_path %>
