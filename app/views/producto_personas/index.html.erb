<p id="notice"><%= notice %></p>

<h1>Listing Producto Personas</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Producto</th>
      <th>Nombre comprador</th>
      <th>Apellido comprador</th>
      <th>Rut comprador</th>
      <th>Digito verifiador comprador</th>
      <th>Email comprador</th>
      <th>Numero comprador</th>
      <th>Nombre asegurado</th>
      <th>Apellido asegurado</th>
      <th>Rut asegurado</th>
      <th>Digito verificador asegurado</th>
      <th>Email asegurado</th>
      <th>Numero asegurado</th>
      <th>Fecha inicio</th>
      <th>Fecha termino</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @producto_personas.each do |producto_persona| %>
      <tr>
        <td><%= producto_persona.producto %></td>
        <td><%= producto_persona.nombre_comprador %></td>
        <td><%= producto_persona.apellido_comprador %></td>
        <td><%= producto_persona.rut_comprador %></td>
        <td><%= producto_persona.digito_verifiador_comprador %></td>
        <td><%= producto_persona.email_comprador %></td>
        <td><%= producto_persona.numero_comprador %></td>
        <td><%= producto_persona.nombre_asegurado %></td>
        <td><%= producto_persona.apellido_asegurado %></td>
        <td><%= producto_persona.rut_asegurado %></td>
        <td><%= producto_persona.digito_verificador_asegurado %></td>
        <td><%= producto_persona.email_asegurado %></td>
        <td><%= producto_persona.numero_asegurado %></td>
        <td><%= producto_persona.fecha_inicio %></td>
        <td><%= producto_persona.fecha_termino %></td>
        <td><%= link_to 'Show', producto_persona %></td>
        <td><%= link_to 'Edit', edit_producto_persona_path(producto_persona) %></td>
        <td><%= link_to 'Destroy', producto_persona, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

