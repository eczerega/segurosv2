<p id="notice"><%= notice %></p>

<h1>Listing Producto Inmuebles</h1>

<table>
  <thead>
    <tr>
      <th>Producto</th>
      <th>Tipo vivienda</th>
      <th>Construccion</th>
      <th>Region</th>
      <th>Ciudad</th>
      <th>Comuna</th>
      <th>Calle</th>
      <th>Numero</th>
      <th>Fecha inicio</th>
      <th>Fecha termino</th>
      <th>Nombre comprador</th>
      <th>Apellido comprador</th>
      <th>Rut comprador</th>
      <th>Digito verificador comprador</th>
      <th>Email comprador</th>
      <th>Numero comprador</th>
      <th>Nombre asegurado</th>
      <th>Apellido asegurado</th>
      <th>Rut asegurado</th>
      <th>Digito verificador asegurado</th>
      <th>Email asegurado</th>
      <th>Numero asegurado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @producto_inmuebles.each do |producto_inmueble| %>
      <tr>
        <td><%= producto_inmueble.producto %></td>
        <td><%= producto_inmueble.tipo_vivienda %></td>
        <td><%= producto_inmueble.construccion %></td>
        <td><%= producto_inmueble.region %></td>
        <td><%= producto_inmueble.ciudad %></td>
        <td><%= producto_inmueble.comuna %></td>
        <td><%= producto_inmueble.calle %></td>
        <td><%= producto_inmueble.numero %></td>
        <td><%= producto_inmueble.fecha_inicio %></td>
        <td><%= producto_inmueble.fecha_termino %></td>
        <td><%= producto_inmueble.nombre_comprador %></td>
        <td><%= producto_inmueble.apellido_comprador %></td>
        <td><%= producto_inmueble.rut_comprador %></td>
        <td><%= producto_inmueble.digito_verificador_comprador %></td>
        <td><%= producto_inmueble.email_comprador %></td>
        <td><%= producto_inmueble.numero_comprador %></td>
        <td><%= producto_inmueble.nombre_asegurado %></td>
        <td><%= producto_inmueble.apellido_asegurado %></td>
        <td><%= producto_inmueble.rut_asegurado %></td>
        <td><%= producto_inmueble.digito_verificador_asegurado %></td>
        <td><%= producto_inmueble.email_asegurado %></td>
        <td><%= producto_inmueble.numero_asegurado %></td>
        <td><%= link_to 'Show', producto_inmueble %></td>
        <td><%= link_to 'Edit', edit_producto_inmueble_path(producto_inmueble) %></td>
        <td><%= link_to 'Destroy', producto_inmueble, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Producto inmueble', new_producto_inmueble_path %>
