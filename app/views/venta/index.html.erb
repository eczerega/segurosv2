<p id="notice"><%= notice %></p>

<h1>Listing Venta</h1>

<table>
  <thead>
    <tr>
      <th>Tipo producto</th>
      <th>Monto</th>
      <th>Vendedor</th>
      <th>Punto de venta</th>
      <th>Aux1</th>
      <th>Aux2</th>
      <th>Aux3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venta.each do |ventum| %>
      <tr>
        <td><%= ventum.tipo_producto %></td>
        <td><%= ventum.monto %></td>
        <td><%= ventum.vendedor_id %></td>
        <td><%= ventum.punto_de_venta_id %></td>
        <td><%= ventum.aux1 %></td>
        <td><%= ventum.aux2 %></td>
        <td><%= ventum.aux3 %></td>
        <td><%= link_to 'Show', ventum %></td>
        <td><%= link_to 'Edit', edit_ventum_path(ventum) %></td>
        <td><%= link_to 'Destroy', ventum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ventum', new_ventum_path %>
