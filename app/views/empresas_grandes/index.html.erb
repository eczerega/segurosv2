<p id="notice"><%= notice %></p>

<h1>Listing Empresas Grandes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Representante legal rut</th>
      <th>Dv</th>
      <th>Aux1</th>
      <th>Aux2</th>
      <th>Aux3</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @empresas_grandes.each do |empresas_grande| %>
      <tr>
        <td><%= empresas_grande.nombre %></td>
        <td><%= empresas_grande.representante_legal_rut %></td>
        <td><%= empresas_grande.dv %></td>
        <td><%= empresas_grande.aux1 %></td>
        <td><%= empresas_grande.aux2 %></td>
        <td><%= empresas_grande.aux3 %></td>
        <td><%= empresas_grande.image %></td>
        <td><%= link_to 'Show', empresas_grande %></td>
        <td><%= link_to 'Edit', edit_empresas_grande_path(empresas_grande) %></td>
        <td><%= link_to 'Destroy', empresas_grande, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Empresas grande', new_empresas_grande_path %>
